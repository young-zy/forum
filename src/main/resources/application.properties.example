# sql database config
spring.datasource.url=jdbc:mysql://127.0.0.1:3306/Forum
spring.jpa.hibernate.ddl-auto=none
spring.datasource.username=         # your username here
spring.datasource.password=         # your password here
spring.jpa.show-sql=true            # should be turned off during production
spring.jpa.database-platform=org.hibernate.dialect.MySQL8Dialect
# let query column name be the name of annotations
spring.jpa.hibernate.naming.physical-strategy=org.hibernate.boot.model.naming.PhysicalNamingStrategyStandardImpl

# token redis config
token.redis.host=127.0.0.1
token.redis.port=6379
token.redis.db-index=0
token.redis.password=       #your password here

# rate limit redis config
rate.redis.host=127.0.0.1
rate.redis.port=6379
rate.redis.password=  #your password here
rate.redis.db-index=1
